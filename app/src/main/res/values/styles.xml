<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorTeal</item>
        <item name="colorPrimaryDark">@color/colorTeal</item>
        <item name="colorAccent">@color/colorTeal</item>
    </style>

    <style name="CustomSwitch" parent="Theme.AppCompat.Light">
        <item name="colorControlActivated">@color/colorTeal</item>
        <item name="colorSwitchThumbNormal">@color/colorGrey400</item>
        <item name="android:colorForeground">@color/colorGrey</item>
    </style>

</resources>
